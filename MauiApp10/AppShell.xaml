<?xml version="1.0" encoding="UTF-8" ?>

<Shell
    x:Class="MauiApp10.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp10"
    Shell.FlyoutBehavior="Flyout"
    Title="MauiApp10">

    <ShellContent
        Icon="ana.png"
        Title="Ana Sayfa"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent 
        Icon="calcu.png"
        Title="KrediHesaplama"
        ContentTemplate="{DataTemplate local:KrediHesaplama}"
        Route="Kredi Hesaplama" />
    <ShellContent 
        Icon="health.png"
    Title="Vucut Kitle Indeksi"
    ContentTemplate="{DataTemplate local:VucutKitleIndeksi}"
    Route="Vucut Kitle Indeksi" />
    <ShellContent 
        Icon="colors.png"
    Title="Renk Secici"
    ContentTemplate="{DataTemplate local:NewPage1}"
    Route="Renk Secici" />
    <ShellContent 
     Icon="address.png"
     Title="Yapilacaklar"
     ContentTemplate="{DataTemplate local:Yapilacaklar}"
     Route="Yapilacaklar" />



</Shell>

